{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"target": "es2017",
		"module": "commonjs",
		"jsx": "react-native",
		"moduleResolution": "Node",
		"noEmit": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"baseUrl": ".",
		"paths": {
			"components/*": ["./src/components/*"],
			"@assets/*": ["./assets/*"],
			"@/*": ["./src/*"],
			"~/*": ["./*"],
			// "@/assets/*": ["./assets/*"]
		}
	},
	"tsx": "react-tsx",
	"jsx": "react-jsx",
	"include": ["**/*.ts", "**/*.tsx", ".expo/type/**/*.ts", ".expo-env.d.ts", "metro.config.js", "src/**/*.ts", "src/**/*.tsx","*.ts", "*.tsx"],
	"exclude": ["node_modules"]
}
